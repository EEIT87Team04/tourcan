<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.tourcan.trip.model.TripVO" table="trip" >
        
        <id name="trip_id" column="trip_id">
            <generator class="identity"/>
        </id>
        
        <property name="trip_name" column="trip_name" />
        <property name="trip_ctime" column="trip_ctime" />
        <property name="trip_price" column="trip_price" />
        <property name="mem_uid" column="mem_uid" />
        
<!--         <many-to-one name="regionVO" class="com.tourcan.region.model.RegionVO" lazy="false"> -->
<!--             <column name="region_id" /> -->
<!--         </many-to-one> -->
    </class>
</hibernate-mapping>

