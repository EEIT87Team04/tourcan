<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.tourcan.resp.model.RespVO" table="resp" >
        
        <id name="resp_id" column="resp_id">
            <generator class="identity"/>
        </id>
        
        <property name="resp_time" column="resp_time" />
        <property name="resp_topic" column="resp_topic" />
        <property name="resp_article" column="resp_article" />
        <property name="theme_id" column="theme_id" />
        <property name="mem_uid" column="mem_uid" />
        
        
<!--         <many-to-one name="regionVO" class="com.tourcan.region.model.RegionVO" lazy="false"> -->
<!--             <column name="region_id" /> -->
<!--         </many-to-one> -->
        
    </class>
</hibernate-mapping>

