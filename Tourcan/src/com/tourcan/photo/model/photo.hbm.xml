<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.tourcan.photo.model.PhotoVO" table="photo" >
        
        <id name="photo_id" column="photo_id">
            <generator class="identity"/>
        </id>
        
        <property name="photo_file" column="photo_file" />
        <property name="att_id" column="att_id" />
        <property name="hotel_id" column="hotel_id" />
        
<!--         <many-to-one name="regionVO" class="com.tourcan.region.model.RegionVO" lazy="false"> -->
<!--             <column name="region_id" /> -->
<!--         </many-to-one> -->
        
    </class>
</hibernate-mapping>

